#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yunkitae/Development/ng-monorepo/node_modules/.pnpm/@nuxt+devtools@1.0.5_nuxt@3.8.2_vite@4.5.1/node_modules/@nuxt/devtools/node_modules:/Users/yunkitae/Development/ng-monorepo/node_modules/.pnpm/@nuxt+devtools@1.0.5_nuxt@3.8.2_vite@4.5.1/node_modules/@nuxt/node_modules:/Users/yunkitae/Development/ng-monorepo/node_modules/.pnpm/@nuxt+devtools@1.0.5_nuxt@3.8.2_vite@4.5.1/node_modules:/Users/yunkitae/Development/ng-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yunkitae/Development/ng-monorepo/node_modules/.pnpm/@nuxt+devtools@1.0.5_nuxt@3.8.2_vite@4.5.1/node_modules/@nuxt/devtools/node_modules:/Users/yunkitae/Development/ng-monorepo/node_modules/.pnpm/@nuxt+devtools@1.0.5_nuxt@3.8.2_vite@4.5.1/node_modules/@nuxt/node_modules:/Users/yunkitae/Development/ng-monorepo/node_modules/.pnpm/@nuxt+devtools@1.0.5_nuxt@3.8.2_vite@4.5.1/node_modules:/Users/yunkitae/Development/ng-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.mjs" "$@"
else
  exec node  "$basedir/../../cli.mjs" "$@"
fi
