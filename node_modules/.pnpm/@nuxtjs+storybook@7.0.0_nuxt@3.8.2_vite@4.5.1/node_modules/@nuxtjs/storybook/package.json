    "typescript": "^5.2.2",
    "unbuild": "^1.2.1",
    "jsdom": "^17.0.0"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "latest",
    "@nuxt/kit": "3.7.3",
    "chalk": "^5.3.0",
    "defu": "^6.1.2",
    "get-port-please": "^3.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
